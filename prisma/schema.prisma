
generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id                   Int     @id @default(autoincrement())
  userKey              String
  from                 String
  to                   String
  amount               String
  txHash               String
  gasLimit             String
  maxFeePerGas         String?   // optional is better
  maxPriorityFeePerGas String?
  gasPrice             String?
  gasUsed              String
  status               String
}



